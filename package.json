{
  "name": "@kamshak/release-notes-generator",
  "description": "Generates the release notes/changelog",
  "author": "Valentin Funk <funk.valentin@gmail.com>; Stephan BÃ¶nnemann <stephan@boennemann.me> (http://boennemann.me)",
  "bugs": {
    "url": "https://github.com/Kamshak/release-notes-generator/issues"
  },
  "config": {
    "nyc": {
      "exclude": [
        ".test",
        "node_modules"
      ]
    }
  },
  "dependencies": {
    "conventional-changelog": "^1.1.3",
    "github-url-from-git": "^1.4.0"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "mkdirp": "^0.5.1",
    "proxyquire": "^1.5.0",
    "rimraf": "^2.4.0",
    "standard": "^4.2.1",
    "semantic-release": "^6.3.2"
  },
  "homepage": "https://github.com/Kamshak/release-notes-generator#readme",
  "keywords": [
    "semantic-release",
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kamshak/release-notes-generator.git"
  },
  "scripts": {
    "prepublish": "build",
    "build": "rimraf dist && mkdirp dist && babel src --out-dir dist",
    "build:tests": "rimraf .test && mkdirp .test && babel test --out-dir .test",
    "test": "npm run test:style",
    "test:style": "standard",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post"
  },
  "version": "0.0.0-development"
}
